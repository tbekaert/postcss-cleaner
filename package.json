{
  "name": "postcss-css-cleaner",
  "version": "0.0.0",
  "description": "PostCSS plugin to clean unused CSS",
  "keywords": ["postcss", "css", "postcss-plugin", "clean", "cleaner"],
  "author": "Thomas <tbekaert@siliconsalad.com>",
  "license": "MIT",
  "repository": "tbekaert/postcss-css-cleaner",
  "bugs": {
    "url": "https://github.com/tbekaert/postcss-css-cleaner/issues"
  },
  "homepage": "https://github.com/tbekaert/postcss-css-cleaner",
  "dependencies": {
    "postcss": "^6.0.1"
  },
  "devDependencies": {
    "eslint": "^4.12.1",
    "eslint-config-postcss": "^2.0.2",
    "jest": "^21.2.1"
  },
  "scripts": {
    "test": "jest && eslint *.js"
  },
  "eslintConfig": {
    "extends": "eslint-config-postcss",
    "env": {
      "jest": true
    },
    "rules": {
      "no-extra-parens": 0,
      "space-before-function-paren": 0,
      "operator-linebreak": 0,
      "max-len": 0
    }
  }
}
